 <div class="container gallery-container">
    <h1>My Storage</h1>   
    <div class="tz-gallery">

        <div class="row">
        {{ $idactive := .pginfo.Iactive }}
         {{range .imagels}}
	            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <a class="lightbox" href="/GalleryDetail/{{.Imageurl}}">
                        <img src="/images/{{.Imageurl}}/image0.jpg" alt="image">
                    </a>
                    <div class="caption">
                        <h3>Thumbnail label</h3>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>
                    </div>
               
                <div align="left">
                    <a href="#" ><i class="fa fa-weixin" ></i></a>
                    <span style="float:right;">
                     <a href="/user/GalleryDel/{{.Imageurl}}?pgid={{$idactive}}"><i class="fa fa-trash" ></i></a>
                    </span>
                </div>
                </div>
            </div>
        {{end}}

        </div>

    </div>
{{if .bshowpagation}}
<nav>
  <ul class="pagination pg-blue">

    {{ $idactive := .pginfo.Iactive }}
    {{if .pginfo.Bprivous}}
        <li class="page-item">
            <a class="page-link" href="/user/dashboard/page/1">First<span class="sr-only">(current)</span></a>
        </li>
        <li class="page-item">
        <a class="page-link" href="/user/dashboard/page/{{$.pginfo.PrevPage}}" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
         </a>
         </li>
    {{else}}
        <li class="page-item disabled">
            <a class="page-link" href="/user/dashboard/page/1">First<span class="sr-only">(current)</span></a>
        </li>
      <li class="page-item disabled">
      <a class="page-link" href="#" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>    
    {{end}}
  
    {{range $i, $e := .pginfo.Pages}}
        {{if eq $e $idactive}}
            <li class="page-item active">
            <a class="page-link" href="/user/dashboard/page/{{$e}}">{{$e}} <span class="sr-only">(current)</span></a>
            </li>
        {{else}}
                <li class="page-item"><a class="page-link" href="/user/dashboard/page/{{$e}}">{{$e}}</a></li>
        {{end}}

    {{end}}

    {{if .pginfo.Bnext}}
         <li class="page-item">
         <a class="page-link" href="/user/dashboard/page/{{.pginfo.NextPage}}" aria-label="Next">
         <span aria-hidden="true">&raquo;</span>
         <span class="sr-only">Next</span>
         </a>
         </li>
         <li class="page-item">
            <a class="page-link" href="/user/dashboard/page/{{.pginfo.Lastpg}}">Last<span class="sr-only">(current)</span></a>
         </li>
    {{else}}
        <li class="page-item disabled">
        <a class="page-link" href="#" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
        </a>
        </li>
         <li class="page-item disabled">
            <a class="page-link" href="/user/dashboard/page/{{.pginfo.Lastpg}}">Last<span class="sr-only">(current)</span></a>
         </li>
    {{end}}

  </ul>
</nav>
{{end}}
